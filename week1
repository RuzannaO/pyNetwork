
def int32_to_ipv4(a):
    b = (f'{a:b}')
    list1 = []
    for i in range(1, 5):
            list1.append(b[int(8*(i-1)):i*8])

    return (list(map(lambda x: int(x, 2), list1)))


def ips_between(a, b):
    return ipv4_to_int(b) - ipv4_to_int(a)


def ipv4_to_int(a):
    list1 = []
    b = a.split(".")
    s = (list(map(int, b)))
    for i in s:
        list1.append(f'{int((i)):08b}')
    return int("".join(list1), 2)
